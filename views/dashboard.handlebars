<div class="bg-dash">
    <div class="prof-marg">
    <div class="search-bar-row-dashboard">
        <input id="inputField" type="text" placeholder="Search.." class="search-bar-dashboard dash-input">
    <button id="search-listen-dashboard" type="submit" id="search-listen-dashboard" class="search-btn-dashboard">Search</button>
    </div>
    <div class="scroll-bar">
        <div class="submit-btn">
  {{#each userDataLookup}}
    <div class="prof-marg">
      <div class="friend-box">
        <div class="prof-align">
          <p class="prof-marg">{{name}}</p>
          <button data-id={{id}} class="friend-request">Add Friend</button>
        </div>
      </div>
    </div>
  {{/each}}
  </div>
  <div class="delete-btn-dashboard">
  {{#each userData.friends as | friend |}}
    <div class="prof-marg">
      <div class="friend-box">
        <div class="prof-align">
          <p class="prof-marg">{{friend.name}}</p>
          <button data-deleteId={{friend.id}} class="friend-delete">Delete Friend</button>
        </div>
      </div>
    </div>
  {{/each}}
  </div>
  </div>
</div>
</div>
<script src="/js/friend.js"></script>