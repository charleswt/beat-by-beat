<div id="profile" class="profile-container">

<h1 class="profile-header">My Favorite Artists</h1>
{{!-- <h2 class="new-text-gradient form-text font-playpen">My Favorite Artists</h2> --}}

{{#if favArtists.length}}
  <ul class="fav-artist">
    {{#each favArtists}}
      <button class="fav-artist-btn artist" data-artistName="{{this}}">{{this}}</button>
    {{/each}}
  </ul>
{{else}}
  <p>You have no favorite artists saved.</p>
{{/if}}

<h1 class="profile-header margin-t">My FriendsList</h1>
{{#if friendsList.length}}
  <ul class="fav-artist">
    {{#each friendsList as |friend|}}
      <button class="fav-artist-btn artist">{{friend.name}}</button>
    {{/each}}
  </ul>
{{/if}}


</div>
<script src="/js/script.js"></script>
